
.agreement-container {
  padding: 30rpx;
  background-color: #fff;
  min-height: 100vh;
}
.title {
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
  text-align: center;
  margin-bottom: 40rpx;
  padding-top: 20rpx;
}
.content {
  margin-bottom: 30rpx;
}
.section {
  margin-bottom: 30rpx;
}
.section-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 20rpx;
}
.section-content {
  padding-left: 10rpx;
}
.paragraph {
  font-size: 28rpx;
  color: #666;
  line-height: 1.6;
  margin-bottom: 20rpx;
  text-align: justify;
}
.list-item {
  font-size: 28rpx;
  color: #666;
  line-height: 1.6;
  margin-bottom: 10rpx;
  padding-left: 20rpx;
}
.footer {
  font-size: 24rpx;
  color: #999;
  text-align: center;
  margin: 40rpx 0;
}
.back-btn {
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #3cc51f;
  color: #fff;
  font-size: 32rpx;
  border-radius: 45rpx;
  margin: 30rpx auto;
}
